APP_NAME=Laravel
APP_ENV=local
# APP_KEY se genera con `php artisan key:generate`
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

# Logging configuration (daily rotation by default)
LOG_CHANNEL=stack
# Comma separated list of channels to include in the stack (daily rotates files)
LOG_STACK=daily
LOG_DEPRECATIONS_CHANNEL=null
# Minimum severity logged by the channels above
LOG_LEVEL=debug
# Number of days to keep daily log files
# LOG_DAILY_DAYS=14

# Database connection (SQLite default for local development)
DB_CONNECTION=sqlite
# SQLite usa un archivo dentro de database/
DB_DATABASE=database/database.sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

# Cache configuration (file driver by default)
CACHE_STORE=file
# CACHE_PREFIX=
# DB_CACHE_CONNECTION=
DB_CACHE_TABLE=cache
# DB_CACHE_LOCK_CONNECTION=
DB_CACHE_LOCK_TABLE=cache_locks

# Queue configuration (database driver by default)
QUEUE_CONNECTION=database
# DB_QUEUE_CONNECTION=
DB_QUEUE_TABLE=jobs
DB_QUEUE=default
DB_QUEUE_RETRY_AFTER=90
# QUEUE_FAILED_DRIVER=database-uuids

# Session storage
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Broadcasting
BROADCAST_CONNECTION=log

# Default filesystem disk (local, public, s3...)
FILESYSTEM_DISK=public
FILESYSTEM_DISK_PRIVATE=private

# In-memory cache hosts (optional)
MEMCACHED_HOST=127.0.0.1

# Redis connection (optional)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail transport configuration
MAIL_MAILER=smtp
MAIL_SCHEME=null
# MAIL_URL=
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
# MAIL_EHLO_DOMAIN=
# MAIL_LOG_CHANNEL=
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS integration (used by some optional services)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
